{
  "manifest_version": 3,
  "name": "Intentional",
  "version": "1.0",
  "description": "Be intentional while you surf the web.",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "options_page": "options.html"
}